{
  "key_bindings": {
    "__global__": {
      "C-c": "quit"
    },
    "__hidden__.__global__": {
      "C-p": "up",
      "C-n": "down",
      "C-b": "left",
      "C-f": "right"
    },
    "__main__": {
      "<UP>": "up",
      "<DOWN>": "down",
      "<LEFT>": "left",
      "<RIGHT>": "right"
    }
  },
  "key_labels": {
    "<UP>": "↑",
    "<DOWN>": "↓",
    "<LEFT>": "←",
    "<RIGHT>": "→"
  },
  "commands": {
    "quit": {
      "type": "quit"
    },
    "up": {
      "type": "move",
      "row": -1
    },
    "down": {
      "type": "move",
      "row": 1
    },
    "left": {
      "type": "move",
      "col": -1
    },
    "right": {
      "type": "move",
      "col": 1
    }
  }
}
