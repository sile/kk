{
  "keybindings": {
    "__main__": {
      "<UP>": "up",
      "<DOWN>": "down",
      "<LEFT>": "left",
      "<RIGHT>": "right"
    }
  },
  "keyremaps": {
    "C-p": "<UP>",
    "C-n": "<DOWN>",
    "C-b": "<LEFT>",
    "C-f": "<RIGHT>"
  },
  "keylabels": {
    "<UP>": "↑",
    "<DOWN>": "↓",
    "<LEFT>": "←",
    "<RIGHT>": "→"
  },
  "commands": {
    "up": {
      "type": "move",
      "row": -1
    },
    "down": {
      "type": "move",
      "row": 1
    },
    "left": {
      "type": "move",
      "col": -1
    },
    "right": {
      "type": "move",
      "col": 1
    }
  }
}
